{
  "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "paths": {
        "@utilities/*": ["./utilities/*"],
        "@routes/*": ["./routes/*"],
        "@config/*": ["./config/*"],
      }
  },
  "include": ["."],
  "exclude": ["node_modules"]
}
